<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".fragments.SearchFragment"
    android:background="#eee">

    <RelativeLayout

        android:layout_width="match_parent"

        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    <RelativeLayout

        android:layout_width="match_parent"

        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_54sdp">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/card1"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            android:background="#FFF"
            app:cardElevation="@dimen/_5sdp"
            app:cardUseCompatPadding="false">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:paddingStart="10dp"
                android:paddingTop="10dp"
                android:paddingEnd="10dp"
                android:paddingBottom="10dp">

                <com.maple.rimaproject.Font.CustomTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Price Range"
                    android:textSize="@dimen/_16sdp"
                    android:textStyle="bold" />



                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp">

                        <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar
                            android:id="@+id/rangeSeekbar6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                           />

                        <TextView
                            android:id="@+id/textMin6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_below="@id/rangeSeekbar6"
                            android:layout_marginTop="5dp"
                            android:layout_marginStart="10dp"
                            android:textSize="16dp"
                            android:textColor="#000"/>

                        <TextView
                            android:id="@+id/textMax6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_below="@id/rangeSeekbar6"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="10dp"
                            android:textSize="16dp"
                            android:textColor="#000"
                            android:layout_alignParentRight="true"/>

                    </RelativeLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:visibility="gone"
                    android:orientation="horizontal"
                   >

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">



                        <com.libizo.CustomEditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:id="@+id/edit_start"
                            android:hint="Minimum Price"

                            android:paddingLeft="15dp"
                            android:paddingTop="12dp"
                            android:paddingRight="12dp"
                            android:paddingBottom="12dp"
                            android:text=""
                            android:gravity="center"
                            android:textColor="@color/colorAccent"
                            android:textSize="15sp"
                            app:libIZO_clearIconTint="@color/colorAccent"
                            app:libIZO_setBorderColor="#d4d2d3"
                            app:libIZO_setBorderView="true"
                            app:libIZO_setClearIconVisible="true"
                            app:libIZO_setCornerRadius="20dp"
                            app:libIZO_setFont="@string/DroidSansBold" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">


                        <com.libizo.CustomEditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:id="@+id/edit_end"
                            android:hint="Maximum Price"

                            android:paddingLeft="15dp"
                            android:paddingTop="12dp"
                            android:paddingRight="12dp"
                            android:paddingBottom="12dp"
                            android:text=""
                            android:gravity="center"
                            android:textColor="@color/colorAccent"
                            android:textSize="15sp"
                            app:libIZO_clearIconTint="@color/colorAccent"
                            app:libIZO_setBorderColor="#d4d2d3"
                            app:libIZO_setBorderView="true"
                            app:libIZO_setClearIconVisible="true"
                            app:libIZO_setCornerRadius="20dp"
                            app:libIZO_setFont="@string/DroidSansBold" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>


        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
